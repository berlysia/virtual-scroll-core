{
  "name": "virtual-scroll-core",
  "version": "0.0.1",
  "description": "height cacheable virtual scroll - core only",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha test/",
    "build": "babel src --out-dir lib --source-maps",
    "watch": "babel src --out-dir lib --watch --source-maps",
    "prepublish": "npm run --if-present changelog && git add CHANGELOG.md && git commit -m 'docs(CHANGELOG): Update' && npm run --if-present doc && npm run --if-present build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "doc:build": "esdoc -c esdoc.json",
    "doc:publish": "gh-pages -d doc",
    "doc": "npm run doc:build && npm run doc:publish"
  },
  "author": "berlysia",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-plugin-add-module-exports": "^0.1.4",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "power-assert": "^1.3.1"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "binary-indexed-tree": "^0.4.0",
    "indexof": "0.0.1",
    "is-array": "^1.0.1",
    "is-window": "^1.0.2",
    "map-like": "^1.0.1",
    "object-keys": "^1.0.9"
  },
  "directories": {
    "doc": "doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/berlysia/virtual-scroll-core.git"
  },
  "bugs": {
    "url": "https://github.com/berlysia/virtual-scroll-core/issues"
  },
  "homepage": "https://github.com/berlysia/virtual-scroll-core#readme"
}
